#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main()
{
   setlocale(LC_ALL, "Portuguese");
    
    int soma = 0;

    
    for (int i = 200; i <= 500; i++) {
        
        if (i % 2 != 0) {
            soma += i;
        }
    }

    printf("A soma dos números ímpares entre 200 e 500 é: %d\n", soma);

   system("pause");
   return 0;
}