#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main()
{
   setlocale(LC_ALL, "Portuguese");
   printf("Vamos mostrar todos os números ímpares entre 50 e 100!");

    for (int i = 51; i <= 100; i += 2) 
    {
        printf("%d\n", i);
    }

   system("pause");
   return 0;
}
