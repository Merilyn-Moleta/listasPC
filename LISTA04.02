#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main()
{
   setlocale(LC_ALL, "Portuguese");
   
    int num;

    printf("Vamos criar a tabuada (multiplicado até 10) do número que escolher, aproveite o programa (∩^o^)⊃\n\n");
    printf("Digite um número para a tabuada: ");
    scanf("%d", &num);
    
    printf("\n\n\nAqui está a tabuada do %d:\n", num);
  
    for (int i = 1; i <= 10; i++) 
    {
        printf("%d x %d = %d\n", num, i, num * i);
    }

   system("pause");
   return 0;
}